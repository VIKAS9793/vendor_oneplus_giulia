service qegahal /odm/bin/vendor.qti.qegahal-service
    class hal
    oneshot
    disabled
    user system
    group system
    interface aidl vendor.qti.qegahal.IQegahal/default

on post-fs-data
    #Create qega-hal deamon related dirs
    mkdir /data/vendor/qega 0770 root system
